import java.util.*;
import java.io.*;

public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int count = 0;
		int num = n;
		
		while(num != 0){
		    count++;
		    num = num/10;
		}
		
		int div = (int)Math.pow(10, count - 1);
		
		while(n != 0){
		    System.out.println(n/div);
		    
		    n = n % div;
		    div = div /10;
		}
	}
}
