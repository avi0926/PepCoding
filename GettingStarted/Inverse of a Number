import java.io.*;
import java.util.*;

public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		int invOfNum = 0;
		
		for (int pos=1; num != 0 ; pos++){
		    
		    int n = num % 10;
		    
		    invOfNum += (pos * (int)Math.pow(10, n-1));
		    
		    num /= 10;
		} 
		System.out.println(invOfNum);
	}
}
